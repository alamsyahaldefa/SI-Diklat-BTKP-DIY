{
  "name": "si-diklat",
  "version": "1.1.1",
  "private": true,
  "scripts": {
    "install-deps": "composer install && npm install && npm install --save-dev postcss autoprefixer file-loader",
    "dev": "npm run development",
    "build": "npm run build-production",
    "development": "mix",
    "watch": "mix watch",
    "watch-poll": "mix watch -- --watch-options-poll=1000",
